{
    "manifest_version": 3,
    "name": "learn-helper",
    "version": "1.0",
    "action": {
        "default_title": "learn-helper",
        "default_popup": "./index.html"
    },
    "permissions": [
        "tabs",
        "activeTab",
        "scripting"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "components/Panel/main.js"
            ]
        }
    ]
}